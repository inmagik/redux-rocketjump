{
  "name": "redux-rocketjump",
  "version": "1.7.2",
  "description": "Rocketjump your redux! Speed up redux-app development",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "repository": "https://github.com/inmagik/redux-rocketjump",
  "authors": [
    "Giovanni Fumagalli <giovanni.fumagalli@inmagik.com>",
    "Mauro Bianchi <mauro.bianchi@inmagik.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inmagik/redux-rocketjump/issues"
  },
  "keywords": [
    "redux",
    "rest",
    "api"
  ],
  "scripts": {
    "format": "prettier --single-quote --trailing-comma es5 --no-semi --write \"{src,__{tests,mocks}__}/**/*.js\"",
    "test": "jest",
    "lint": "eslint src",
    "start": "npm-run-all -p dev fake-api",
    "dev": "webpack-dev-server",
    "fake-api": "json-server --watch --delay 400 example/db.json",
    "build": "rimraf lib && rollup -c && node scripts/make-plugin-defs.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.x",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "classnames": "^2.2.5",
    "copyfiles": "^2.0.0",
    "css-loader": "^0.28.10",
    "eslint": "5.x",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "jest": "^23.1.0",
    "json-server": "^0.12.2",
    "npm-run-all": "^4.1.2",
    "prettier": "1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-saga": "^0.16.0",
    "redux-saga-thunk": "^0.7.1",
    "regenerator-runtime": "^0.11.1",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "style-loader": "^0.20.2",
    "superagent": "^3.8.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "peerDependencies": {
    "redux": "^3.0.0 || ^4.0.0",
    "redux-saga": ">=0.15.0",
    "reselect": ">=3.0.0"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "lodash.get": "^4.4.2",
    "lodash.mapvalues": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "object-path-immutable": "^3.0.0",
    "query-string": "5"
  },
  "sideEffects": false
}
